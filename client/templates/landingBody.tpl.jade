.navbar-fixed
  nav.transparent(role="navigation")
    .nav-wrapper.container
      a#logo-container.brand-logo(href="/")
        img(src="/branding/jupitlogo_sidebyside_white.png")
      
      a.button-collapse(href="#" data-activates="mobile-nav")
        i.material-icons menu
      
      ul.right.hide-on-med-and-down
        if currentUser
          li
            a(href="#{profileUrl}") Profile
        a.btn(href="/sign-up") Sign Up

      ul.side-nav#mobile-nav
        if currentUser
          li
            a(href="#{profileUrl}") Profile
        a.btn(href="/sign-up") Sign Up
        //+atNavButton

+yield
